	@RequestMapping(value = "deleteFamily", method = RequestMethod.POST)
	@ResponseBody
	public Set<Pravasi> deletePravasi(@SessionAttribute(value = "pravasiSet", required = false) Set<Pravasi> pravasiSet,
			@RequestParam(value = "id") int id) {
		int i = 1;
		Pravasi pravasi = new Pravasi();
		for (Pravasi pravasi1 : pravasiSet) {
			if (i == id) {
				pravasi = pravasi1;
				break;
			}
			i++;
		}
		pravasiSet.remove(pravasi);
		return pravasiSet;
	}

