	@RequestMapping(value = "getDemesticAppicationByMobile", method = RequestMethod.POST)
	@ResponseBody
	public long getDemesticAppicationByMobile(@RequestParam(value = "mobile") String mobile,
			@RequestParam("captchaCode") String captcha, HttpSession session, HttpServletRequest request) {
		if (session.getAttribute("captcha_security").equals(captcha)) {
			session.removeAttribute("captcha_security");
			
			if(domesticService.getCountOfDomestic(mobile)>0) {
				sendMessage(mobile, request);
				return 1;
			}
		} else {
			session.removeAttribute("captcha_security");
			return -1;
		}
		return 0;
	}

