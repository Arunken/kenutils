	@RequestMapping(value = "/captcha", method = RequestMethod.GET)
	@ResponseBody
	public void index(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		response.setContentType("image/jpg");
		int iTotalChars = 6;
		int iHeight = 40;
		int iWidth = 170;
		Font fntStyle1 = new Font("monotype corsiva", Font.BOLD, 30);
		/* Random randChars = new Random(); */
		String sImageCode = commonService.getRandomNumberString();
		BufferedImage biImage = new BufferedImage(iWidth, iHeight, BufferedImage.TYPE_INT_RGB);
		Graphics2D g2dImage = (Graphics2D) biImage.getGraphics();
		int iCircle = 15;
		boolean change = true;
		for (int i = 0; i < iCircle; i++) {
			if (change) {
				change = false;
				g2dImage.setColor(new Color(228, 0, 107));
			} else {
				change = true;
				g2dImage.setColor(new Color(0, 0, 0));
			}
		}
		g2dImage.setFont(fntStyle1);
		g2dImage.setBackground(new Color(196, 225, 255));
		g2dImage.clearRect(0, 0, iWidth, iHeight);
		for (int i = 0; i < iTotalChars; i++) {
			if (change) {
				change = false;
				g2dImage.setColor(new Color(228, 0, 107));
			} else {
				change = true;
				g2dImage.setColor(new Color(0, 0, 0));
			}
			if (i % 2 == 0) {
				g2dImage.drawString(sImageCode.substring(i, i + 1), 25 * i, 24);
			} else {
				g2dImage.drawString(sImageCode.substring(i, i + 1), 25 * i, 35);
			}
		}
		OutputStream osImage = response.getOutputStream();
		ImageIO.write(biImage, "jpeg", osImage);
		g2dImage.dispose();
		HttpSession session = request.getSession();
		session.setAttribute("captcha_security", sImageCode);
	}

	@RequestMapping(value = "checkLogin", method = RequestMethod.POST)
	@ResponseBody
	public char checkLogin(@RequestParam("captchaCode") String captcha, UserDetails userDetails, HttpSession session) {
		if (!session.getAttribute("captcha_security").equals(captcha)) {
			session.removeAttribute("captcha_security");
			return '1';
		}
		return '0';
	}

