	@RequestMapping("/")
	public ModelAndView home(ModelAndView model) {
		model.setViewName("homePage");

		HitCount hitCount = masterService.getHitCount();
		Long hit = hitCount.getHit();
		hit += 1;
		hitCount.setHit(hit);
		masterService.saveHitCount(hitCount);
		model.addObject("hit", hit);
		model.addObject("dashBoard", coronaService.listDashBoard());
		District kozhikode = masterService.getDistrict(8);
		long orgQur = coronaService.countPatientQuentine(kozhikode)
				- coronaService.countReleasedPatient(kozhikode, new Date());
		model.addObject("orgQur", orgQur);
		return model;
	}

