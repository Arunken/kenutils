	@RequestMapping(value = "loginReq", method = RequestMethod.GET)
	public ModelAndView login(ModelAndView model, HttpServletRequest request,
			@SessionAttribute("username") String username) {
		UserDetails ud = userDetailsService.getUserDetails(username);
		ud.setAttempt(0);
		userDetailsService.saveUserDetails(ud);
		HttpSession session = request.getSession();
		if (ud.getType().equals("NIC")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/nic/");
		} else if (ud.getType().equals("RRT")) {
			RapidResponseTeam rapidResponseTeam = coronaService.getRapidResponseTeam(ud);
			session.setAttribute("rrt", rapidResponseTeam);
			model.setViewName("redirect:/corona/");
		} else if (ud.getType().equals("DC")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/");
		} else if (ud.getType().equals("MO")) {
			session.setAttribute("mOfficer", coronaService.getMedicalOfficer(ud));
			model.setViewName("redirect:/medicalOfficer/");
		} else if (ud.getType().equals("DO")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/");
		} else if (ud.getType().equals("CU")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/addDashBoard");
		} else if (ud.getType().equals("LB")) {
			session.setAttribute("localbody", masterService.getLocalBody(ud));
			model.setViewName("redirect:/localBody/");
		} else if (ud.getType().equals("TO")) {
			session.setAttribute("taluk", masterService.getTaluk(ud));
			model.setViewName("redirect:/taluk/");
		} else if (ud.getType().equals("DMO")) {
			session.setAttribute("dmo", masterService.getDMO(ud));
			model.setViewName("redirect:/dmoRto/");
		} else if (ud.getType().equals("RTO")) {
			session.setAttribute("rto", masterService.getRTO(ud));
			model.setViewName("redirect:/dmoRto/");
		} else if (ud.getType().equals("STM")) {
			session.setAttribute("stm", coronaService.getSTM(ud));
			model.setViewName("redirect:/stm/");
		} else if (ud.getType().equals("VO")) {
			session.setAttribute("village", villageService.getVillage(ud));
			model.setViewName("redirect:/village/");
		} else if (ud.getType().equals("VUE")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/akshaya/");
		} else if (ud.getType().equals("SO")) {
			session.setAttribute("sOfficer", masterService.getStateOfficer(ud));
			model.setViewName("redirect:/state/");
		} else if (ud.getType().equals("FDC")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/");
		} else if (ud.getType().equals("CDMO")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/");
		} else if (ud.getType().equals("ADMO")) {
			session.setAttribute("officer", masterService.getOfficer(ud));
			model.setViewName("redirect:/user/");
		} else if (ud.getType().equals("HO")) {
			session.setAttribute("hospital", masterService.getHospital(ud));
			model.setViewName("redirect:/hospital/");
		} else if (ud.getType().equals("JD")) {
			session.setAttribute("jnrDr", coronaService.getJuniorDoctor(ud));
			model.setViewName("redirect:/juniorDoctor/");
		} else if (ud.getType().equals("AP")) {
			session.setAttribute("airUser", pravasiService.getAirportUser(ud));
			model.setViewName("redirect:/airport/");
		} else if (ud.getType().equals("NO")) {
			session.setAttribute("nOfficer", pravasiService.getNodalOfficer(ud));
			model.setViewName("redirect:/nodal/");
		} else if (ud.getType().equals("CP")) {
			session.setAttribute("chkUser", domesticService.getCheckPostUser(ud));
			model.setViewName("redirect:/checkpost/listDomestic");
		} else if (ud.getType().equals("SOP")) {
			session.setAttribute("sOfficer", masterService.getStateOfficer(ud));
			model.setViewName("redirect:/state/listAllPatient");
		} else if (ud.getType().equals("STA")) {
			session.setAttribute("sOfficer", masterService.getStateOfficer(ud));
			model.setViewName("redirect:/state/trainTravelerList");
		} else if (ud.getType().equals("FSTA")) {
			session.setAttribute("sOfficer", masterService.getStateOfficer(ud));
			model.setViewName("redirect:/state/trainTravelerList");
		}
		session.setAttribute("user", ud);

		LoginLog prevLog = userDetailsService.getLastLoginLog(ud);
		if (prevLog != null) {
			session.setAttribute("prevLog", prevLog);
		} else {
			session.setAttribute("prevLog", new LoginLog());
		}
		LoginLog loginLog = new LoginLog();
		loginLog.setUserDetails(ud);
		loginLog.setLoginTime(new Date());
		UserAgent userAgent = UserAgent.parseUserAgentString(request.getHeader("User-Agent"));
		loginLog.setIp(request.getRemoteHost());
		loginLog.setBrowser(userAgent.getBrowser().getName());
		loginLog.setOs(userAgent.getOperatingSystem().getName());
		userDetailsService.saveLoginLog(loginLog);
		session.setAttribute("log", loginLog);
		return model;
	}

	private boolean isCurrentAuthenticationAnonymous() {
		final Authentication authentication = SecurityContextHolder.getContext().getAuthentication();
		return authenticationTrustResolver.isAnonymous(authentication);
	}

	@RequestMapping("logout")
	public ModelAndView logout(ModelAndView model, HttpSession session,
			@SessionAttribute(value = "log", required = false) LoginLog log) {
		if (log != null) {
			log.setLogoutTime(new Date());
			userDetailsService.saveLoginLog(log);
		}
		session.removeAttribute("log");
		session.removeAttribute("prevLog");
		session.removeAttribute("user");
		session.removeAttribute("mOfficer");
		session.removeAttribute("rrt");
		session.removeAttribute("officer");
		session.removeAttribute("taluk");
		session.removeAttribute("localbody");
		session.removeAttribute("user");

		session.invalidate();
		Authentication auth = SecurityContextHolder.getContext().getAuthentication();
		if (auth != null) {
			SecurityContextHolder.getContext().setAuthentication(null);
		}
		model.setViewName("redirect:/home/loginPage?logout");
		return model;
	}

