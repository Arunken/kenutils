	@RequestMapping(value = "addOffence")
	public ModelAndView addOffence(ModelAndView model) {
		model.addObject("districtList", masterService.listDistrict());
		model.addObject("offenceCatagory", otherService.listOffenceCatagory());
		model.addObject("offence", new Offence());
		model.setViewName("offence");
		return model;
	}

	@RequestMapping(value = "saveOffence", method = RequestMethod.POST)
	public ModelAndView saveOffence(@ModelAttribute("offence") Offence offence,
			@RequestParam(value = "mobile") String mobile,
			@RequestParam(value = "lattitude", required = false, defaultValue = "") String lattitude,
			@RequestParam(value = "longitude", required = false, defaultValue = "") String longitude,
			@RequestParam(value = "image", required = false) CommonsMultipartFile attachment,
			@RequestParam(value = "offenceCatagoryId", required = false, defaultValue = "0") long offenceCatagoryId,
			RedirectAttributes redirectAttributes,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") long localBodyId,
			HttpServletRequest request) {
		
		if(otherService.countofOffence(mobile)== 0 && 
				!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
			redirectAttributes.addFlashAttribute("message", "Invalid Otp");
			return new ModelAndView("redirect:/home/addOffence");
		}
		offence.setOffenceCatagory(otherService.getOffenceCatagory(offenceCatagoryId));
		
		if (ESAPI.validator().isValidInput("Mobile", offence.getMobile(), "Mobile", 10, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
			return new ModelAndView("redirect:/home/addOffence");
		}
		if (ESAPI.validator().isValidInput("localBodyId", String.valueOf(localBodyId), "Num", 4, false) == false
				|| localBodyId == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Local Body");
			return new ModelAndView("redirect:/home/addOffence");
		}
		if (offence.getOffenceCatagory() == null || ESAPI.validator().isValidInput("Offence Category",
				String.valueOf(offence.getOffenceCatagory().getOffenceCatagoryId()), "Num", 2, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Offence Category");
			return new ModelAndView("redirect:/home/addOffence");
		}
		if (ESAPI.validator().isValidInput("Landmark", offence.getLandmark(), "WordS", 255, false) == false) {
			redirectAttributes.addFlashAttribute("message",
					"Invalid Landmark. Only alphabets, digits and space allowed(255 characters max)");
			return new ModelAndView("redirect:/home/addOffence");
		}
		
		if (attachment != null && attachment.getSize() > 0) {
			String[] supportedMimeTypes = {"image/jpeg","application/pdf"};
			String errors="";
			try {
				errors = commonService.validateMultipartFile(attachment, supportedMimeTypes, 500l);
			} catch (Exception e1) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. " + errors);
				return new ModelAndView("redirect:/home/addOffence");
			}
			if (null == errors || !errors.trim().equals("")) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. " + errors);
				return new ModelAndView("redirect:/home/addOffence");
			}
			if (attachment.getSize() > 0) {
				offence.setAttachment(attachment.getBytes());
			}
		}
		if (ESAPI.validator().isValidInput("Latitude", lattitude, "Latitude", 20, true) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Latitude");
			return new ModelAndView("redirect:/home/addOffence");
		}
		if (ESAPI.validator().isValidInput("Longitude", longitude, "Longitude", 20, true) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Longitude");
			return new ModelAndView("redirect:/home/addOffence");
		}
		
		if (ESAPI.validator().isValidInput("Description", offence.getDescription(), "WordSS", 2000, false) == false) {
			redirectAttributes.addFlashAttribute("message",
					"Invalid Description. Only alphabets, digits, space and the following special characters are allowed(-_,:/@#.$!*'%^()) (2000 characters max)");
			return new ModelAndView("redirect:/home/addOffence");
		}

		offence.setLattitude(lattitude);
		offence.setLongitude(longitude);
		offence.setLocalBody(masterService.getLocalBody(localBodyId));
		offence.setOffenceDate(new Date());
		otherService.saveOffence(offence);

		redirectAttributes.addFlashAttribute("message", "Offence Saved Successfully");
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "getOffence", method = RequestMethod.POST)
	@ResponseBody
	public long getOffence(@RequestParam(value = "mobile") String mobile, 
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {
		
		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			session.removeAttribute("captcha_security");
			if(otherService.countofOffence(mobile) == 0) {
				sendMessage(session, mobile);
					return 0;
				}
			else {
				return 1;
			}
		} else {
			session.removeAttribute("captcha_security");
			return -1;
		}
	}

