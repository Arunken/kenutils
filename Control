	@RequestMapping(value = "addStaff", method = RequestMethod.POST)
	@ResponseBody
	public Set<PersonalDetails> addMoreApplicant(@ModelAttribute("personalDetails") PersonalDetails personalDetails,
			@SessionAttribute(value = "staffSet", required = false) Set<PersonalDetails> staffSet) {
		staffSet.add(personalDetails);
		return staffSet;
	}

	@RequestMapping(value = "deleteStaff", method = RequestMethod.POST)
	@ResponseBody
	public Set<PersonalDetails> deleteApplicant(
			@SessionAttribute(value = "staffSet", required = false) Set<PersonalDetails> staffSet,
			@RequestParam(value = "id") int id) {
		int i = 1;
		PersonalDetails personalDetails = new PersonalDetails();
		for (PersonalDetails person : staffSet) {
			if (i == id) {
				personalDetails = person;
				break;
			}
			i++;
		}
		staffSet.remove(personalDetails);
		return staffSet;
	}

	@RequestMapping(value = "saveShopPermit", method = RequestMethod.POST)
	public ModelAndView saveShopPermit(RedirectAttributes redirectAttributes,
			@ModelAttribute("shopPermit") ShopPermit shopPermit,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") long localBodyId,
			@RequestParam(value = "exemptionId", required = false, defaultValue = "0") long exemptionId,
			@SessionAttribute(value = "staffSet", required = false) Set<PersonalDetails> staffSet) {
		shopPermit.setLocalBody(masterService.getLocalBody(localBodyId));
		shopPermit.setStaffs(staffSet);
		shopPermit.setExemption(coronaService.getExemption(exemptionId));
		coronaService.saveShopPermit(shopPermit);
		String msgContent = "";
		try {
			msgContent = "Covid 19 Jagratha: Your application has been send for processing. You will receive SMS after approval. You can check status by below link\n"
					+ "https://covid19jagratha.kerala.nic.in/home/viewStatus?shopPermitId=" + URLEncoder
							.encode(commonService.encryptMessage(Long.toString(shopPermit.getShopPermitId())), "UTF-8");
		} catch (UnsupportedEncodingException e) {
			e.printStackTrace();
		}
		commonService.sendSMS(shopPermit.getOwnerMobile(), msgContent, false);
		redirectAttributes.addFlashAttribute("message",
				"Your application has been send for processing. You will receive SMS after approval.");
		return new ModelAndView("redirect:/home/");
	}

