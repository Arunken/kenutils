	@RequestMapping(value = "saveDomesticEntry", method = RequestMethod.POST)
	public ModelAndView saveDomesticEntry(@ModelAttribute("member") Domestic domestic,
			@SessionAttribute(value = "domesticSet", required = false) Set<Domestic> domesticSet,
			@RequestParam(value = "localBodyId", required = false) long localBodyId,
			@RequestParam(value = "vehicleCapacityId") long vehicleCapacityId,
			@RequestParam(value = "reasonId", required = false) long reasonId,
			@RequestParam(value = "timeSlotId", required = false) long timeSlotId,
			@RequestParam(value = "stateId") long stateId,
			@RequestParam(value = "distOtherId", required = false, defaultValue = "0") long distOtherId,
			@RequestParam(value = "stationId", required = false, defaultValue = "0") long stationId,
			@RequestParam(value = "trainId", required = false, defaultValue = "0") long trainId,
			@RequestParam(value = "coachId", required = false, defaultValue = "0") long coachId,
			RedirectAttributes redirectAttributes, HttpServletRequest request) {
		TimeSlot timeSlot = domesticService.getTimeSlot(timeSlotId);
		domestic.setLocalBody(masterService.getLocalBody(localBodyId));
		domestic.setApplicationDate(new Date());
		domestic.setVehicleCapacity(domesticService.getVehicleCapacity(vehicleCapacityId));
		domestic.setReturnReason(domesticService.getReturnReason(reasonId));
		domestic.setTimeSlot(timeSlot);
		domestic.setState(masterService.getState(stateId));
		domestic.setDistrictOther(masterService.getDistrictOther(distOtherId));
		RailwayStation railwayStation = domesticService.getRailwayStation(stationId);
		Train train = domesticService.getTrain(trainId);
		Coach coach = domesticService.getCoach(coachId);
		domestic.setRailwayStation(railwayStation);
		domestic.setTrain(train);
		domestic.setCoach(coach);
		for (Domestic domestic2 : domesticSet) {
			domestic2.setApplicationDate(new Date());
			domestic2.setHead(domestic);
			domestic2.setVehicleCapacity(domestic.getVehicleCapacity());
			domestic2.setVehicleNo(domestic.getVehicleNo());
			domestic2.setReturnReason(domestic.getReturnReason());
			domestic2.setState(domestic.getState());
			domestic2.setDepartingPlace(domestic.getDepartingPlace());
			domestic2.setTravelDate(domestic.getTravelDate());
			domestic2.setTimeSlot(domestic.getTimeSlot());
			domestic2.setDistrictOther(domestic.getDistrictOther());
			domestic2.setRailwayStation(railwayStation);
			domestic2.setDepartingDate(domestic.getDepartingDate());
			domestic2.setTrain(train);
			domestic2.setCoach(coach);
		}
		domesticSet.add(domestic);
		
		for(Domestic dom : domesticSet) {
			if(!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
				redirectAttributes.addFlashAttribute("message", "Invalid Otp");
				return new ModelAndView("redirect:/home/domesticNewEntry");
			}
			if (ESAPI.validator().isValidInput("Mobile", dom.getMobile(), "Mobile", 10, false) == false) {
				redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
				return new ModelAndView("redirect:/home/domesticNewEntry");
			}
			
			if(dom.getDob() != null) {
					try {
						SimpleDateFormat sdf = new SimpleDateFormat("E MMM dd HH:mm:ss z yyyy");
						sdf.setLenient(false);
						sdf.parse(String.valueOf(String.valueOf(dom.getDob())));
					}catch(Exception ex){
						redirectAttributes.addFlashAttribute("message", "Invalid DOB");
						return new ModelAndView("redirect:/home/domesticNewEntry");
					}
			}else {
				redirectAttributes.addFlashAttribute("message", "Invalid DOB");
				return new ModelAndView("redirect:/home/domesticNewEntry");
			}
			
			
		}
		
		if (domestic.getVehicleCapacity().getCapacity() != 0) {
			if (domesticSet.size() > timeSlot.getCapacity()) {
				ModelAndView model = new ModelAndView();
				model.addObject("domesticSet", domesticSet);
				model.addObject("domestic", domestic);
				model.addObject("capacityList", domesticService.listVehicleCapacity());
				model.addObject("checkpostList", domesticService.listCheckPost(domestic.getLocalBody().getDistrict()));
				model.setViewName("redirect:/home/addNewDomestic");
				model.addObject("message", "Time slot you choosen is filled. Please choose another.");
				return model;
			}
		}
		if (domesticService.saveDomestic(domesticSet) == domesticSet.size()) {
			if (domestic.getVehicleCapacity().getCapacity() != 0) {
				timeSlot.setCapacity(timeSlot.getCapacity() - domesticSet.size());
				domesticService.saveTimeSlot(timeSlot);
			}
			String msgContent = "";
			try {
				msgContent = "Covid 19 Jagratha: Your application with number " + domestic.getDomesticId()
						+ " has been send for processing. You will receive SMS after approval. You can check status by below link\n"
						+ "https://covid19jagratha.kerala.nic.in/viewStatus?domesticId=" + URLEncoder
								.encode(commonService.encryptMessage(Long.toString(domestic.getDomesticId())), "UTF-8");
			} catch (UnsupportedEncodingException e) {
				e.printStackTrace();
			}
			commonService.sendSMS(domestic.getMobile(), msgContent, false);
			redirectAttributes.addFlashAttribute("message",
					"Your application has been send for processing. Your registration number is "
							+ domestic.getDomesticId());
		} else {
			redirectAttributes.addFlashAttribute("message", "Some problem occurs. Please try again.");
		}

		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "verifyDomesticPermit")
	public ModelAndView verifyDomesticPermit(@RequestParam(value = "domesticId") String domesticId,
			RedirectAttributes redirectAttributes) {
		Domestic domestic = domesticService.getDomestic(Long.parseLong(commonService.decryptMessage(domesticId)));
		if (domestic != null) {
			if (domestic.getStatus() == 'A') {
				ModelAndView model = new ModelAndView();
				model.addObject("domestic", domestic);
				model.setViewName("verification");
				return model;
			} else {
				redirectAttributes.addFlashAttribute("message", "This pass is not approved by concern authority");
			}
		} else {
			redirectAttributes.addFlashAttribute("message", "This is not a valid pass");
		}
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "deleteDomestic", method = RequestMethod.POST)
	@ResponseBody
	public Set<Domestic> deleteDomestic(
			@SessionAttribute(value = "domesticSet", required = false) Set<Domestic> domesticSet,
			@RequestParam(value = "id") int id) {
		int i = 1;
		Domestic domestic = new Domestic();
		for (Domestic domestic1 : domesticSet) {
			if (i == id) {
				domestic = domestic1;
				break;
			}
			i++;
		}
		domesticSet.remove(domestic);
		return domesticSet;
	}

