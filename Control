	@RequestMapping(value = "getTimeSlot", method = RequestMethod.POST)
	@ResponseBody
	public List<TimeSlot> getTimeSlot(@RequestParam(value = "travelDate") String travelDate,
			@RequestParam(value = "checkPostId") long checkPostId) {
		Date date = new Date();
		DateFormat format = new SimpleDateFormat("dd-MM-yyyy");
		try {
			date = format.parse(travelDate);
		} catch (ParseException e) {
			e.printStackTrace();
		}
		List<TimeSlot> timeSlots = null;
		CheckPost checkPost = domesticService.getCheckPost(checkPostId);
		if (checkPost.getType() == 'L') {
			timeSlots = domesticService.getTimeSlot(date, checkPost);
		} else if (checkPost.getType() == 'S') {
			timeSlots = domesticService.getTimeSlot(checkPost);
		} else if (checkPost.getType() == 'R') {
			timeSlots = domesticService.getTimeSlot(checkPost);
		}
		if (timeSlots == null) {
			timeSlots = new ArrayList<TimeSlot>();
			TimeSlot timeSlot1 = new TimeSlot();
			timeSlot1.setCheckPost(checkPost);
			timeSlot1.setCapacity(checkPost.getSlot1());
			timeSlot1.setSlot(domesticService.getSlot(1));
			timeSlot1.setDate(date);
			timeSlots.add(timeSlot1);
			TimeSlot timeSlot2 = new TimeSlot();
			timeSlot2.setCheckPost(checkPost);
			timeSlot2.setCapacity(checkPost.getSlot2());
			timeSlot2.setSlot(domesticService.getSlot(2));
			timeSlot2.setDate(date);
			timeSlots.add(timeSlot2);
			TimeSlot timeSlot3 = new TimeSlot();
			timeSlot3.setCheckPost(checkPost);
			timeSlot3.setCapacity(checkPost.getSlot3());
			timeSlot3.setSlot(domesticService.getSlot(3));
			timeSlot3.setDate(date);
			timeSlots.add(timeSlot3);
			TimeSlot timeSlot4 = new TimeSlot();
			timeSlot4.setCheckPost(checkPost);
			timeSlot4.setCapacity(checkPost.getSlot4());
			timeSlot4.setSlot(domesticService.getSlot(4));
			timeSlot4.setDate(date);
			timeSlots.add(timeSlot4);
			domesticService.saveTimeSlot(timeSlots);
		} else if (timeSlots.size() == 0) {
			timeSlots = new ArrayList<TimeSlot>();
			TimeSlot timeSlot1 = new TimeSlot();
			timeSlot1.setCheckPost(checkPost);
			timeSlot1.setCapacity(checkPost.getSlot1());
			timeSlot1.setSlot(domesticService.getSlot(1));
			timeSlot1.setDate(date);
			timeSlots.add(timeSlot1);
			TimeSlot timeSlot2 = new TimeSlot();
			timeSlot2.setCheckPost(checkPost);
			timeSlot2.setCapacity(checkPost.getSlot2());
			timeSlot2.setSlot(domesticService.getSlot(2));
			timeSlot2.setDate(date);
			timeSlots.add(timeSlot2);
			TimeSlot timeSlot3 = new TimeSlot();
			timeSlot3.setCheckPost(checkPost);
			timeSlot3.setCapacity(checkPost.getSlot3());
			timeSlot3.setSlot(domesticService.getSlot(3));
			timeSlot3.setDate(date);
			timeSlots.add(timeSlot3);
			TimeSlot timeSlot4 = new TimeSlot();
			timeSlot4.setCheckPost(checkPost);
			timeSlot4.setCapacity(checkPost.getSlot4());
			timeSlot4.setSlot(domesticService.getSlot(4));
			timeSlot4.setDate(date);
			timeSlots.add(timeSlot4);
			domesticService.saveTimeSlot(timeSlots);
		}
		return timeSlots;
	}

	@RequestMapping(value = "saveDomestic", method = RequestMethod.POST)
	public ModelAndView saveDomestic(@ModelAttribute("domestic") Domestic domestic1,
			@RequestParam(value = "memberId", required = false) long[] memberId, RedirectAttributes redirectAttributes,
			@RequestParam(value = "timeSlotId") long timeSlotId,
			@RequestParam(value = "stateId", required = false, defaultValue = "0") long stateId,
			@RequestParam(value = "vehicleCapacityId") long vehicleCapacityId) {
		State state = masterService.getState(stateId);
		TimeSlot timeSlot = domesticService.getTimeSlot(timeSlotId);
		Domestic domestic = domesticService.getDomestic(domestic1.getDomesticId());
		domestic.setTravelDate(domestic1.getTravelDate());
		domestic.setVehicleNo(domestic1.getVehicleNo());
		domestic.setTimeSlot(timeSlot);
		domestic.setVehicleCapacity(domesticService.getVehicleCapacity(vehicleCapacityId));
		domestic.setState(state);
		Set<Domestic> members = new HashSet<>();
		members.add(domestic);
		if (memberId != null) {
			for (long member : memberId) {
				Domestic domestic2 = domesticService.getDomestic(member);
				domestic2.setTravelDate(domestic.getTravelDate());
				domestic2.setVehicleNo(domestic.getVehicleNo());
				domestic2.setVehicleCapacity(domestic.getVehicleCapacity());
				domestic2.setTimeSlot(timeSlot);
				domestic2.setHead(domestic);
				domestic2.setState(state);
				members.add(domestic2);
			}
		}
		if (domestic.getVehicleCapacity().getCapacity() != 0) {
			if (members.size() > timeSlot.getCapacity()) {
				ModelAndView model = new ModelAndView();
				model.addObject("members", members);
				model.addObject("domestic", domestic);
				model.addObject("capacityList", domesticService.listVehicleCapacity());
				model.addObject("checkpostList", domesticService.listCheckPost(domestic.getLocalBody().getDistrict()));
				model.setViewName("domestic/domestic");
				model.addObject("message", "Time slot you choosen is filled. Please choose another.");
				return model;
			}
		}
		if (domesticService.saveDomestic(members) == members.size()) {
			if (domestic.getVehicleCapacity().getCapacity() != 0) {
				timeSlot.setCapacity(timeSlot.getCapacity() - members.size());
				domesticService.saveTimeSlot(timeSlot);
			}
			String msgContent = "";
			try {
				msgContent = "Covid 19 Jagratha: Your application with number " + domestic.getDomesticId()
						+ " has been send for processing. You will receive SMS after approval. You can check status by below link\n"
						+ "https://covid19jagratha.kerala.nic.in/viewStatus?domesticId=" + URLEncoder
								.encode(commonService.encryptMessage(Long.toString(domestic.getDomesticId())), "UTF-8");
			} catch (UnsupportedEncodingException e) {
				e.printStackTrace();
			}
			commonService.sendSMS(domestic.getMobile(), msgContent, false);
			redirectAttributes.addFlashAttribute("message",
					"Your application has been send for processing. Your registration number is "
							+ domestic.getDomesticId());
		} else {
			redirectAttributes.addFlashAttribute("message", "Some problem occurs. Please try again.");
		}
		return new ModelAndView("redirect:/home/");
	}

