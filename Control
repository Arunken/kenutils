	}

	@RequestMapping(value = "saveMedicalEmergencyPass", method = RequestMethod.POST, consumes = {
			MediaType.MULTIPART_FORM_DATA_VALUE })
	public ModelAndView saveMedicalEmergencyPass(
			@ModelAttribute("medicalEmergencyPass") MedicalEmergencyPass medicalEmergencyPass,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") long localBodyId,
			RedirectAttributes redirectAttributes, @RequestParam(value = "mobile") String mobile,
			@RequestParam(value = "attach", required = false) CommonsMultipartFile attachment,
			@RequestParam(value = "fromStateId", required = false, defaultValue = "0") long fromStateId,
			@RequestParam(value = "toStateId", required = false, defaultValue = "0") long toStateId,
			@RequestParam(value = "toDistId", required = false, defaultValue = "0") long toDistId,
			@RequestParam(value = "fromDistId", required = false, defaultValue = "0") long fromDistId, 
			HttpServletRequest request) {
		
		if(!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
			redirectAttributes.addFlashAttribute("message", "Invalid Otp");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Mobile", mobile, "Mobile", 10, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("localBodyId", String.valueOf(localBodyId), "Num", 4, false) == false
				|| localBodyId == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Localbody");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Name", medicalEmergencyPass.getName(), "Surname",20, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Name");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Address", medicalEmergencyPass.getAddress(), "WordSS", 200, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid address. "
					+ "Only alphabets, digits, space and the following special characters "
					+ "are allowed(-_,:/@#.$!*'%^()) (200 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (medicalEmergencyPass.getIdType() == null || medicalEmergencyPass.getIdType().trim().equals("")) {
			redirectAttributes.addFlashAttribute("message", "Invalid Id Type.");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (medicalEmergencyPass.getIdNo() == null || medicalEmergencyPass.getIdNo().trim().equals("") || ESAPI.validator().isValidInput("IDNo", 
				medicalEmergencyPass.getIdNo(), "WordS", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Id. Only alphanumeric values are allowed (max 50 characters)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Purpose", String.valueOf(medicalEmergencyPass.getPurpose()), 
				"WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Purpose. Only alphabets, digits, space and hyphen are allowed (15 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Vehicle Number", String.valueOf(medicalEmergencyPass.getVehicleNo()),
				"WordSWithHyphen", 15, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Vehicle Number. Only alphabets, digits, space and hyphen are allowed (15 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if(medicalEmergencyPass.getFromDate() != null && medicalEmergencyPass.getToDate() != null) {
			if(medicalEmergencyPass.getFromDate().compareTo(medicalEmergencyPass.getToDate()) < 0) {
				try {
					SimpleDateFormat sdf = new SimpleDateFormat("E MMM dd HH:mm:ss z yyyy");
					sdf.setLenient(false);
					sdf.parse(String.valueOf(String.valueOf(medicalEmergencyPass.getFromDate())));
					sdf.parse(String.valueOf(String.valueOf(medicalEmergencyPass.getToDate())));
				}catch(Exception ex){
					redirectAttributes.addFlashAttribute("message", "Invalid Date");
					return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
				}
			}
			else {
				redirectAttributes.addFlashAttribute("message", "Invalid Date. Return back date should be greater than going date");
				return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
			}
		}else {
			redirectAttributes.addFlashAttribute("message", "Invalid Date");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("From", medicalEmergencyPass.getFrom(), "WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Place From. Only alphabets, digits, space and hyphen are allowed (50 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("To", medicalEmergencyPass.getTo(), "WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Place To. Only alphabets, digits, space and hyphen are allowed (50 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		
		if (ESAPI.validator().isValidInput("Route", medicalEmergencyPass.getRoute(), "WordSWithHyphen", 100, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Route. Only alphabets, digits, space and hyphen are allowed (100 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("Total Passengers", medicalEmergencyPass.getPersonCount(), "Num", 4, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Total No. of passengers. Only digits are allowed");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (ESAPI.validator().isValidInput("TravellerAddress", medicalEmergencyPass.getFellowTraveller(), "WordSS", 200, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Traveller address. "
					+ "Only alphabets, digits, space and the following special characters "
					+ "are allowed(-_,:/@#.$!*'%^()) (200 characters max)");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (fromDistId <= 0) {
			redirectAttributes.addFlashAttribute("message", "Select From District");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		if (toDistId <= 0) {
			redirectAttributes.addFlashAttribute("message", "Select To District");
			return new ModelAndView("redirect:/home/addMedicalEmergencyPass");
		}
		medicalEmergencyPass.setLocalBody(masterService.getLocalBody(localBodyId));
		medicalEmergencyPass.setMobile(mobile);
		medicalEmergencyPass.setApplicationDate(new Date());

		if (fromStateId == 1) {
			District fromDistrict = masterService.getDistrict(fromDistId);
			medicalEmergencyPass.setFromDistrict(fromDistrict);
		} else {
			DistrictOther fromDistrictOther = masterService.getDistrictOther(fromDistId);
			medicalEmergencyPass.setFromDistrictOther(fromDistrictOther);
		}

		if (toStateId == 1) {
			District toDistrict = masterService.getDistrict(toDistId);
			medicalEmergencyPass.setToDistrict(toDistrict);
		} else {
			DistrictOther toDistrictOther = masterService.getDistrictOther(toDistId);
			medicalEmergencyPass.setToDistrictOther(toDistrictOther);
		}
		if (attachment != null && attachment.getSize() > 0) {
			String[] supportedMimeTypes = {"image/jpeg","application/pdf"};
			String errors="";
			try {
				errors = commonService.validateMultipartFile(attachment, supportedMimeTypes, 500l);
			} catch (Exception e1) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. "+errors);
				return new ModelAndView("redirect:/home/");
			}
			if(null==errors||!errors.trim().equals("")) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. "+errors);
				return new ModelAndView("redirect:/home/");
			}
			if (attachment.getSize() > 0) {
				medicalEmergencyPass.setAttachment(attachment.getBytes());
				medicalEmergencyPass.setFileName(attachment.getOriginalFilename());
				medicalEmergencyPass.setAttachType(attachment.getContentType());
			}
		}
		coronaService.saveMedicalEmergencyPass(medicalEmergencyPass);
		String msgContent = "";
		try {
			msgContent = "Covid 19 Jagratha: Your application has been send for processing. You will receive SMS after approval. You can check status by below link\n"
					+ "https://covid19jagratha.kerala.nic.in/home/viewStatus?medicalEmergencyPassId="
					+ URLEncoder.encode(commonService
							.encryptMessage(Long.toString(medicalEmergencyPass.getMedicalEmergencyPassId())), "UTF-8");
		} catch (UnsupportedEncodingException e) {
			e.printStackTrace();
		}
		commonService.sendSMS(medicalEmergencyPass.getMobile(), msgContent, false);
		redirectAttributes.addFlashAttribute("message",
				"Your application has been send for processing. You will receive SMS after approval.");
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "printEmergencyPermit")
	public void printEmergencyPermit(@RequestParam("medicalEmergencyPassId") String medicalEmergencyPassId,
			HttpServletResponse response, HttpServletRequest request) {
		MedicalEmergencyPass medicalEmergencyPass = coronaService
				.getMedicalEmergencyPass(Long.parseLong(commonService.decryptMessage(medicalEmergencyPassId)));

		String content = "";

		if (medicalEmergencyPass != null) {
			if (medicalEmergencyPass.getStatus() == 'A') {
				String qrContent = "";
				try {
					qrContent = "https://covid19jagratha.kerala.nic.in/home/verifyEmergencyPermit?medicalEmergencyPassId="
							+ URLEncoder.encode(commonService.encryptMessage(
									Long.toString(medicalEmergencyPass.getMedicalEmergencyPassId())), "UTF-8");
				} catch (UnsupportedEncodingException e1) {
					e1.printStackTrace();
				}

				String dcSign = "<br /><br />";

				OfficerSign officerSign = masterService.getDCSign(medicalEmergencyPass.getLocalBody().getDistrict());
				if (officerSign != null) {
					dcSign = "<div style=\"text-align:center;\"><img alt=\"qr\"  width=\"80\" height=\"60\" src=\"data:image/png;base64,"
							+ Base64.getEncoder().encodeToString(officerSign.getAttachment()) + "\"/></div><br />";
				}

				String apremrk = "";
				if (medicalEmergencyPass.getApproveRemark() != null) {
					if (!medicalEmergencyPass.getApproveRemark().equals("")) {
						apremrk = "<tr><td>District Collector Remark</td><td>" + medicalEmergencyPass.getApproveRemark()
								+ "</td></tr>";
					}
				}
				String qr = "<img alt=\"qr\"  width=\"100\" height=\"70\" src=\"data:image/png;base64,"
						+ Base64.getEncoder().encodeToString(commonService.generateQRCode(qrContent)) + "\"/>";
				String header = "<table style=\"width: 100%; margin-bottom: 20px;\"><tr><td style=\"text-align: left;\">"
						+ qr + "</td><td style=\"text-align: center;\"><img width=\"120\" height=\"70\""
						+ "src=\" data:image/png;base64,\">"
						+ "</td><td style=\"text-align: right;\"><img width=\"120\" height=\"50\"src=\"data:image/jpeg;base64,\">"
						+ "</td></tr></table>";
				String returnDate = "";
				if (medicalEmergencyPass.getToDate() != null) {
					returnDate = new SimpleDateFormat("dd/MM/yyyy hh:mm aa").format(medicalEmergencyPass.getToDate());
				} else {
					returnDate = "No return";
				}
				content = "<div style=\"font-size: 12pt; max-width: 21cm; margin: 2mm 6mm 8mm 6mm;\">" + header
						+ "<p style=\"text-align: center; font-weight: bold; font-size: 13pt;\">"
						+ "COVID 19 - EMERGENCY SERVICES VEHICLE PASS <br />DISTRICT ADMINISTRATION "
						+ medicalEmergencyPass.getLocalBody().getDistrict().getDistName().toUpperCase()
						+ "- KERALA </p>" + "<p style=\"text-align: center; font-size: 11pt;\">Issued under "
						+ "Section 2 of the Epidemic Diseases act 1897 &amp; The Disaster "
						+ "Management Act 2005 Inter State/Inter District Permit</p>"
						+ "<br/><div style=\"text-align: left; float: left\">Permit Number: "
						+ medicalEmergencyPass.getMedicalEmergencyPassId() + "/2020</div>"
						+ "<div style=\"text-align: left; float: right\">Date: "
						+ new SimpleDateFormat("dd/MM/yyyy").format(medicalEmergencyPass.getApprovedDate()) + "</div>"
						+ "<p style=\"text-align: left; font-size: 13pt; line-height: 30px\"><table"
						+ " cellspacing=\"10\" >" + "<tr><td><b>Applicant Name:</b></td><td><b>"
						+ medicalEmergencyPass.getName() + "</b></td></tr>" + "<tr><td>Address:</td><td>"
						+ medicalEmergencyPass.getAddress() + "</td></tr>"
						+ "<tr><td>Government Issued ID Card:</td><td>" + medicalEmergencyPass.getIdType()
						+ "</td></tr>" + "<tr><td>Government Issued ID Card Number:</td><td>"
						+ medicalEmergencyPass.getIdNo() + "</td></tr><tr><td>No. of Passenger: </td><td>"
						+ medicalEmergencyPass.getPersonCount()
						+ "</td></tr><tr><td>Fellow Traveller(s) and Address: </td><td>"
						+ medicalEmergencyPass.getFellowTraveller() + "</td></tr>"
						+ "<tr><td><b>Purpose of Travel:</b></td><td><b>" + medicalEmergencyPass.getPurpose()
						+ "</b></td></tr>" + "<tr><td>FROM:</td><td>" + medicalEmergencyPass.getFrom() + "</td></tr>"
						+ "<tr><td>To:</td><td>" + medicalEmergencyPass.getTo() + "</td></tr>"
						+ "<tr><td>Going Date &amp; Time:</td><td>"
						+ new SimpleDateFormat("dd/MM/yyyy hh:mm aa").format(medicalEmergencyPass.getFromDate())
						+ "</td></tr>" + "<tr><td>Return Date &amp; Time:</td><td>" + returnDate
						+ "</td></tr><tr><td>Vehicle number:</td><td>" + medicalEmergencyPass.getVehicleNo()
						+ "</td></tr>" + apremrk + "</table>" + " </p><br /> <br />"
						+ "<div style=\"text-align: left; float: right\">" + dcSign
						+ "DISTRICT COLLECTOR &amp; DISTRICT MAGISTRATE<br />"
						+ medicalEmergencyPass.getLocalBody().getDistrict().getDistName().toUpperCase() + "</div>"
						+ "</div>";
			} else {
				content = "<h1>Covid 19 Jagratha: Invalid Emergency Permit.</h1>";
			}
		} else {
			content = "<h1>Covid 19 Jagratha: Invalid Emergency Permit.</h1>";
		}
		try {
			ByteArrayOutputStream byteArrayOutputStream = new ByteArrayOutputStream();
			InputStream inputStream = new ByteArrayInputStream(content.getBytes());
			HtmlConverter.convertToPdf(inputStream, byteArrayOutputStream);
			response.setContentType("application/pdf");
			response.setHeader("Content-Disposition", "attchment;filename=\"Emergency Permit.pdf\"");
			FileCopyUtils.copy(byteArrayOutputStream.toByteArray(), response.getOutputStream());
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	@RequestMapping(value = "verifyEmergencyPermit")
	public ModelAndView verifyEmergencyPermit(
			@RequestParam(value = "medicalEmergencyPassId") String medicalEmergencyPassId,
			RedirectAttributes redirectAttributes) {
		MedicalEmergencyPass medicalEmergencyPass = coronaService
				.getMedicalEmergencyPass(Long.parseLong(commonService.decryptMessage(medicalEmergencyPassId)));
		if (medicalEmergencyPass != null) {
			if (medicalEmergencyPass.getStatus() == 'A') {
				ModelAndView model = new ModelAndView();
				model.addObject("medicalEmergencyPass", medicalEmergencyPass);
				model.setViewName("verification");
				return model;
			} else {
				redirectAttributes.addFlashAttribute("message", "This pass is not approved by concern authority");
			}
		} else {
			redirectAttributes.addFlashAttribute("message", "This is not a valid pass");
		}
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "addSurveillanceDashboard", method = { RequestMethod.GET, RequestMethod.POST })
	public ModelAndView addSurveillanceDashboard(
			@RequestParam(value = "date1", required = false, defaultValue = "") String datestring1,
			@RequestParam(value = "date2", required = false, defaultValue = "") String datestring2,
			@SessionAttribute(value = "reportDate1", required = false) Date reportDate1,
			@SessionAttribute(value = "reportDate2", required = false) Date reportDate2,
			@RequestParam(value = "distId", defaultValue = "0", required = false) long distId,
			HttpServletRequest request,RedirectAttributes redirectAttributes) {
		HttpSession session = request.getSession();
		ModelAndView model = new ModelAndView();
		if(request.getMethod().equalsIgnoreCase("POST")) {
			String captcha = request.getParameter("ncaptchaCode")== null? "":request.getParameter("ncaptchaCode");
			if (!session.getAttribute("captcha_security").equals(captcha)) {
				session.removeAttribute("captcha_security");
				redirectAttributes.addFlashAttribute("message", "Invalid Captcha!");
				model.setViewName("surveillanceDashboard");
				return model;
			}
			
		}
		
		
		Date fromDate = new Date();
		Date toDate = new Date();
		
		DateFormat format = new SimpleDateFormat("dd-MM-yyyy");

		if (!datestring1.equals("")) {
			try {
				fromDate = format.parse(datestring1);
				toDate = format.parse(datestring2);
				session.setAttribute("reportDate1", fromDate);
				session.setAttribute("reportDate2", toDate);
			} catch (ParseException e) {
				fromDate = new Date();
			}
		} else if (reportDate1 != null) {
			fromDate = reportDate1;
			toDate = reportDate2;
		}
		District district = masterService.getDistrict(distId);
		model.addObject("district", district);
		model.addObject("quren", coronaService.countPatientQuentine(district));
		model.addObject("release", coronaService.countReleasedPatient(district, fromDate));
		model.addObject("newEntry", coronaService.countNewEntryPatient(district, fromDate));
		model.addObject("symptom", coronaService.countSuspectWithSymptom(district, fromDate, toDate));
		model.addObject("atHome", coronaService.countSuspectIsAtHome(district, fromDate, toDate));
		model.addObject("moniterd", coronaService.countSuspectWithMoniterd(district, fromDate, toDate));
		model.addObject("moniterdPolice", coronaService.countSuspectWithMoniterdPolice(district, fromDate, toDate));
		model.setViewName("surveillanceDashboard");
		return model;
	}

