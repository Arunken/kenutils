			@RequestParam(value = "mobile", required = false, defaultValue = "0") String mobile) {
		return coronaService.getSannadhasena(mobile);

	}
	//added for captcha check
	@RequestMapping(value = "getVolunteers", method = RequestMethod.POST)
	@ResponseBody
	public long getVolunteers(@RequestParam(value = "mobile") String mobile, 
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {
		
		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			if(coronaService.getSannadhasena(mobile) != null) {
				return 1;
			}else {
				sendMessage(session, mobile);
				return 0;
			}
		}
		else {
			session.removeAttribute("captcha_security");
			return -1;
		}
		
	}
	
	@RequestMapping(value = "getExistingVolunteers", method = RequestMethod.POST)
	@ResponseBody
	public long getExistingVolunteers(@RequestParam(value = "mobile") String mobile, 
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {
		
		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			if(coronaService.getSannadhasena(mobile) != null) {
				sendMessage(session, mobile);
				return 1;
			}else {
				return 0;
			}
		}
		else {
			session.removeAttribute("captcha_security");
			return -1;
		}
		
	}

