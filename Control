	@RequestMapping(value = "saveComments", method = RequestMethod.POST, consumes = {
			MediaType.MULTIPART_FORM_DATA_VALUE })
	public ModelAndView saveComments(@RequestParam(value = "programId") long programId,
			@RequestParam(value = "commnetsId", required = false, defaultValue = "0") long commnetsId,
			@RequestParam(value = "mobile") String mobile, @RequestParam(value = "comment") String comment,
			@RequestParam("image") MultipartFile file, @RequestParam(value = "rating") int rating,
			@RequestParam(value = "name") String name, RedirectAttributes redirectAttributes) {
		Comments comments = null;
		String[] supportedMimeTypes = {"application/pdf","image/jpeg"};
		String errors="";
		try {
			errors = commonService.validateMultipartFile(file,supportedMimeTypes,500l);
		} catch (Exception e1) {
			redirectAttributes.addFlashAttribute("message", "Failed to upload file");
			return new ModelAndView("redirect:/home/");
		}
		if(null==errors||!errors.trim().equals("")) {
			redirectAttributes.addFlashAttribute("message", errors);
			return new ModelAndView("redirect:/home/");
		}else {
			if (masterService.getComments(commnetsId) != null) {
				comments = masterService.getComments(commnetsId);
			} else {
				comments = new Comments();
			}
			comments.setPrograms(fundService.getPrograms(programId));
			comments.setComment(comment);
			comments.setMobile(mobile);
			comments.setVerified(true);
			comments.setRating(rating);
			comments.setDate(new Date());
			comments.setName(name);
			long imageId = 0;
			if (masterService.getMaxCommentsId() != 0) {
	
				imageId = masterService.getMaxCommentsId();
			}
			try {
				
				imageId++;
				byte[] bytes = file.getBytes();
				String rootPath = System.getProperty("catalina.home");
				File dir = new File(rootPath + File.separator + "supportImages");
				if (!dir.exists())
					dir.mkdirs();
	
				File serverFile = new File(dir.getAbsolutePath() + File.separator + "commentImage" + imageId + "."
						+ FilenameUtils.getExtension(file.getOriginalFilename()));
				BufferedOutputStream stream = new BufferedOutputStream(new FileOutputStream(serverFile));
				stream.write(bytes);
				stream.close();
	
				comments.setImageName(
						"commentImage" + imageId + "." + FilenameUtils.getExtension(file.getOriginalFilename()));
	
			} catch (Exception e) {
				e.printStackTrace();
			}
	
			masterService.saveComments(comments);
			redirectAttributes.addFlashAttribute("message", "saved");
		
		}
		
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "getUser", method = RequestMethod.POST)
	@ResponseBody
	public Comments getUser(@RequestParam(value = "mobile") String mobile,
			@RequestParam(value = "programId") long programId) {
		long cnt = masterService.countOfComments(mobile);

		if (cnt != 0) {
			Comments comments = masterService.getComments(mobile, fundService.getPrograms(programId));
			if (comments == null) {
				return new Comments();
			} else {
				return comments;
			}
		}
		return null;

	}

