	@RequestMapping(value = "saveComplaints", method = RequestMethod.POST, consumes = {
			MediaType.MULTIPART_FORM_DATA_VALUE })
	public ModelAndView saveComplaints(@ModelAttribute("complaints") Complaints complaints,
			@RequestParam(value = "image", required = false) CommonsMultipartFile attachment,
			RedirectAttributes redirectAttributes, @RequestParam(value = "localBodyId", 
			required = false, defaultValue = "0") long localBodyId, HttpServletRequest request) {
		
		if(coronaService.countofComplaints(complaints.getMobile())== 0 && 
				!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
			redirectAttributes.addFlashAttribute("message", "Invalid Otp");
			return new ModelAndView("redirect:/home/addComplaints");
		}
		if (ESAPI.validator().isValidInput("Mobile", complaints.getMobile(), "Mobile", 10, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
			return new ModelAndView("redirect:/home/addOffence");
		}
		if (ESAPI.validator().isValidInput("localBodyId", String.valueOf(localBodyId), "Num", 4, false) == false
				|| localBodyId == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Local Body");
			return new ModelAndView("redirect:/home/addComplaints");
		}
		if (ESAPI.validator().isValidInput("Landmark", complaints.getLocation(), "WordS", 255, false) == false) {
			redirectAttributes.addFlashAttribute("message",
					"Invalid Landmark. Only alphabets, digits and space allowed(255 characters max)");
			return new ModelAndView("redirect:/home/addOffence");
		}
		
		if (attachment != null && attachment.getSize() > 0) {
			String[] supportedMimeTypes = {"image/jpeg","image/png"};
			String errors="";
			try {
				errors = commonService.validateMultipartFile(attachment, supportedMimeTypes, 500l);
			} catch (Exception e1) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. "+errors);
				return new ModelAndView("redirect:/home/addComplaints");
			}
			if(null==errors||!errors.trim().equals("")) {
				redirectAttributes.addFlashAttribute("message", "Failed to upload file. "+errors);
				return new ModelAndView("redirect:/home/addComplaints");
			}
			if (attachment.getSize() > 0) {
				complaints.setAttachment(commonService.resize(attachment.getBytes(), 640, 480));
				complaints.setImageName(attachment.getOriginalFilename());
				complaints.setFileType(attachment.getContentType());
			}
		}	
		if (ESAPI.validator().isValidInput("Description", complaints.getRemark(), "WordSS", 2000, false) == false) {
			redirectAttributes.addFlashAttribute("message",
					"Invalid Description. Only alphabets, digits, space and the following special characters are allowed(-_,:/@#.$!*'%^()) (2000 characters max)");
			return new ModelAndView("redirect:/home/addOffence");
		}
		
		complaints.setLocalBody(masterService.getLocalBody(localBodyId));
		complaints.setComplaintDate(new Date());
		coronaService.saveComplaints(complaints);
		redirectAttributes.addFlashAttribute("message", "Compliants Saved Successfully");
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "getComplaints", method = RequestMethod.POST)
	@ResponseBody
	public long getComplaints(@RequestParam(value = "mobile") String mobile,
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {
		
		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			if(coronaService.countofComplaints(mobile) == 0) {
				sendMessage(session, mobile);
				return 0;
			}
			else {
				return 1;
			}
		}
		else {
			session.removeAttribute("captcha_security");
			return -1;
		}
	}

