	@RequestMapping(value = "userValidation", method = RequestMethod.POST)
	@ResponseBody
	public String userValidation(@RequestParam("username") String username,
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {

		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			UserDetails userDetails = userDetailsService.getUserDetails(username);
			if (userDetails != null) {
				session.removeAttribute("captcha_security");
				return userDetails.getUsername();
			}
		}
		else {
			session.removeAttribute("captcha_security");
		}
		return null;
	}

	@RequestMapping(value = "checkOtp", method = RequestMethod.POST)
	@ResponseBody
	public char checkOtp(@RequestParam("otp") String otp, HttpServletRequest request) {
		HttpSession session = request.getSession();
		if (session.getAttribute("otp").equals(otp)) {
			return '1';
		}
		return '0';
	}
	
	// to avoid otp bypassing
	private Boolean checkValidatedOtp(HttpSession session, String otp) {
		if (session.getAttribute("otp")!=null && session.getAttribute("otp").equals(otp)) {
			return true;
		}
		return false;
	}

	@RequestMapping(value = "setForgotPassword", method = RequestMethod.POST)
	public ModelAndView setForgotPassword(@RequestParam("username") String username,
			@RequestParam(value = "password", required = false) String password, RedirectAttributes redirectAttributes,
			HttpServletRequest request) {
		ModelAndView model = new ModelAndView();
		Calendar calendar = Calendar.getInstance();
		calendar.add(Calendar.MONTH, 6);
		String pass = commonService.aesDecrypt(request.getSession(), password);
		if (null != pass && ESAPI.validator().isValidInput("Password", pass, "Password", 15, false) == true) {
			UserDetails userDetails = userDetailsService.getUserDetails(username);
			userDetails.setPassword(commonService.encodePassword(pass));
			userDetails.setNextReset(calendar.getTime());
			if (userDetailsService.saveUserDetails(userDetails) > 0) {
				redirectAttributes.addFlashAttribute("message", "password reset completed");
			} else {
				redirectAttributes.addFlashAttribute("message",
						"Invalid password. Password should contain atleast one Capital Letter, one Small Letter, one Number ,one Special Character from [. _ % ' \\ - @ ! # ^ * $] and Length should be atleast 8 and should not exceed 15");
			}
			model.setViewName("redirect:/home/loginPage");
		} else {
			redirectAttributes.addFlashAttribute("message",
					"Invalid password. Password should contain atleast one Capital Letter, one Small Letter, one Number ,one Special Character from [. _ % ' \\ - @ ! # ^ * $] and Length should be atleast 8 and should not exceed 15");
			model.setViewName("redirect:/home/loginPage");
			
		}
		
		return model;

	}

