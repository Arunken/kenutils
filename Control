	}

	@RequestMapping(value = "editVolunteer", method = RequestMethod.POST)
	public ModelAndView editVolunteer(@ModelAttribute("sannadhasena") Sannadhasena sannadhasena,
			HttpServletResponse response, HttpServletRequest request,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") long localBodyId,
			RedirectAttributes redirectAttributes,@RequestParam(value = "mobile") String mobile) {
		
		if(!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
			redirectAttributes.addFlashAttribute("message", "Invalid Otp");
			return new ModelAndView("redirect:/home/addUpdateVolunteer");
		}
		
		Sannadhasena sannadhasenaold = coronaService.getSannadhasena(sannadhasena.getSannadhasenaId());
		sannadhasena.setLocalBody(masterService.getLocalBody(localBodyId));
		sannadhasena.setSannadhasenaPurpose(sannadhasenaold.getSannadhasenaPurpose());
		sannadhasena.setVillage(sannadhasenaold.getVillage());
		
		if (ESAPI.validator().isValidInput("Mobile", mobile, "Mobile", 10, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (ESAPI.validator().isValidInput("localBodyId", String.valueOf(localBodyId), "Num", 4, false) == false
				|| localBodyId == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Localbody From");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (ESAPI.validator().isValidInput("Name", sannadhasena.getFirstName(), "Surname",15, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid First Name");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		/*if (ESAPI.validator().isValidInput("Name", sannadhasena.getLastName(), "Surname",15, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid LastName");
			return new ModelAndView("redirect:/home/addVolunteer");
		}*/
		if (ESAPI.validator().isValidInput("Sex", sannadhasena.getSex(), "Alpha",15, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Gender");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if(sannadhasena.getDob() != null) {
			if(Period.between(sannadhasena.getDob().toInstant().atZone(ZoneId.systemDefault()).toLocalDate(), 
					(new Date()).toInstant().atZone(ZoneId.systemDefault()).toLocalDate()).getYears() >= 18) {
				try {
					SimpleDateFormat sdf = new SimpleDateFormat("E MMM dd HH:mm:ss z yyyy");
					sdf.setLenient(false);
					sdf.parse(String.valueOf(String.valueOf(sannadhasena.getDob())));
				}catch(Exception ex){
					redirectAttributes.addFlashAttribute("message", "Invalid DOB");
					return new ModelAndView("redirect:/home/addVolunteer");
				}
			}
			else {
				redirectAttributes.addFlashAttribute("message", "Invalid DOB. The volunteer should be of minimum 18 years of age");
				return new ModelAndView("redirect:/home/addVolunteer");
			}
		}else {
			redirectAttributes.addFlashAttribute("message", "Invalid DOB");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (ESAPI.validator().isValidInput("Pincode", sannadhasena.getPinCode(), "Pincode", 6, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid PinCode (max 10 characters)");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (ESAPI.validator().isValidInput("Email", sannadhasena.getEmail(), "Email", 100, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Email Address. Only alphanumeric and the following special characters are allowed (._%'-) (max 100 characters)");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (ESAPI.validator().isValidInput("Qualification", sannadhasena.getEducation(), "WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Educational Qualification. Only alphanumeric, spaces and hyphen are allowed(Max 50 characters)");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (sannadhasena.getIdType() == null) {
			redirectAttributes.addFlashAttribute("message", "Invalid Id Type.");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (sannadhasena.getIdNo() == null || ESAPI.validator().isValidInput("IDNo", 
				sannadhasena.getIdNo(), "WordsWithNum", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Id. Only alphanumeric values are allowed (max 50 characters)");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		if (sannadhasena.getLanguage() == null || ESAPI.validator().isValidInput("Languages", 
				sannadhasena.getLanguage(), "AlphaSComma", 200, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Languages known cannot be left blank. Only alpha values are allowed(max 200 characters)");
			return new ModelAndView("redirect:/home/addVolunteer");
		}
		
		sannadhasena.setEdit(true);
		sannadhasena.setEditDate(new Date());
		coronaService.saveSannadhasena(sannadhasena);

		redirectAttributes.addFlashAttribute("message", "Volunteer details updated successfully.");
		return new ModelAndView("redirect:/home/");
	}

