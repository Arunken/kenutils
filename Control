	@RequestMapping(value = "renewEssentialservices", method = RequestMethod.POST)
	public ModelAndView renewEssentialservices(ModelAndView model,
			@RequestParam(value = "serviceId", required = false, defaultValue = "0") long serviceId,
			@RequestParam(value = "item", required = false, defaultValue = "") String item,
			@RequestParam(value = "quantity", required = false, defaultValue = "") String quantity,
			@RequestParam(value = "goingWith", required = false, defaultValue = "") String goingWith,
			@RequestParam(value = "returnBackDate", required = false, defaultValue = "") String returnBackDate,
			@RequestParam(value = "quarentine", required = false, defaultValue = "FALSE") Boolean quarentine,
			RedirectAttributes redirectAttributes) {
		EssentialService essentialService = coronaService.getEssentialService(serviceId);
		RenewEssentialService renewEssentialService = new RenewEssentialService();
		renewEssentialService.setAttachment(essentialService.getAttachment());
		renewEssentialService.setEssentialService(essentialService);
		renewEssentialService.setGoingWith(essentialService.getGoingWith());
		renewEssentialService.setItem(essentialService.getItem());
		renewEssentialService.setQuantity(essentialService.getQuantity());
		renewEssentialService.setReturnBack(essentialService.getReturnBack());
		Date date = new Date();
		DateFormat format = new SimpleDateFormat("dd-MM-yyyy hh:mm aa");
		try {
			date = format.parse(returnBackDate);
		} catch (ParseException e) {
			date = new Date();
		}
		essentialService.setGoingDate(essentialService.getReturnBack());
		essentialService.setReturnBack(date);
		essentialService.setItem(item);
		essentialService.setQuantity(quantity);
		essentialService.setGoingWith(goingWith);
		essentialService.setQuarentine(quarentine);
		essentialService.setAttachment(null);
		essentialService.setRtoStatus('U');
		essentialService.setDmoStatus('U');
		essentialService.setStatus('U');
		coronaService.renewEssentialService(renewEssentialService);
		commonService.sendSMS(essentialService.getMobile(),
				"Covid 19 Jagratha: Your application for renew permit is send for processing. You will receive SMS after approval.",
				false);
		redirectAttributes.addFlashAttribute("message",
				"Your application for renew permit is send for processing. You will receive SMS after approval.");
		model.setViewName("redirect:/home/");
		return model;
	}

