			response.setContentType("application/pdf");
			response.setHeader("Content-Disposition", "attchment;filename=\"Domestic Permit.pdf\"");
			FileCopyUtils.copy(byteArrayOutputStream.toByteArray(), response.getOutputStream());
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

