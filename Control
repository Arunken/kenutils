	@RequestMapping(value = "checkDomestic", method = RequestMethod.POST)
	@ResponseBody
	public Object checkDomestic(@RequestParam("captchaCode") String captcha, HttpSession session,
			@RequestParam(value = "domesticId") long domesticId) {
		if (!session.getAttribute("captcha_security").equals(captcha)) {
			session.removeAttribute("captcha_security");
			return '1';
		} else {
			Domestic domestic = domesticService.getDomestic(domesticId);
			if (domestic == null) {
				return '2';
			} else if (domestic.getHead() != null) {
				return '3';
			} else {
				return domestic;
			}
		}
	}

