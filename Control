	@RequestMapping(value = "viewStatus")
	public ModelAndView viewStatus(@RequestParam(value = "serviceId", required = false) String serviceId,
			@RequestParam(value = "shopPermitId", required = false) String shopPermitId,
			@RequestParam(value = "medicalEmergencyPassId", required = false) String medicalEmergencyPassId,
			@RequestParam(value = "domesticId", required = false) String domesticId,
			RedirectAttributes redirectAttributes) {
		ModelAndView model = new ModelAndView();
		if (serviceId != null) {
			EssentialService essentialService = coronaService
					.getEssentialService(Long.parseLong(commonService.decryptMessage(serviceId)));
			if (essentialService != null) {
				model.addObject("essentialService", essentialService);
			} else {
				redirectAttributes.addFlashAttribute("message", "This is not a valid vehicle permit");
				return new ModelAndView("redirect:/home/");
			}
		} else if (shopPermitId != null) {
			ShopPermit shopPermit = coronaService
					.getShopPermit(Long.parseLong(commonService.decryptMessage(shopPermitId)));
			if (shopPermit != null) {
				model.addObject("shopPermit", shopPermit);
			} else {
				redirectAttributes.addFlashAttribute("message", "This is not a valid staff permit.");
				return new ModelAndView("redirect:/home/");
			}
		} else if (medicalEmergencyPassId != null) {
			MedicalEmergencyPass medicalEmergencyPass = coronaService
					.getMedicalEmergencyPass(Long.parseLong(commonService.decryptMessage(medicalEmergencyPassId)));
			if (medicalEmergencyPass != null) {
				model.addObject("medicalEmergencyPass", medicalEmergencyPass);
			} else {
				redirectAttributes.addFlashAttribute("message", "This is not a valid emergency travel pass.");
				return new ModelAndView("redirect:/home/");
			}
		} else if (domesticId != null) {
			Domestic domestic = domesticService.getDomestic(Long.parseLong(commonService.decryptMessage(domesticId)));
			if (domestic != null) {
				model.addObject("domestic", domestic);
			} else {
				redirectAttributes.addFlashAttribute("message", "This is not a valid domestic returnees pass.");
				return new ModelAndView("redirect:/home/");
			}
		}
		model.setViewName("status");
		return model;
	}

