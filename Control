	@RequestMapping(value = "saveEssentialservices", method = RequestMethod.POST)
	public ModelAndView saveEssentialservices(@ModelAttribute("essentialService") EssentialService essentialService,
			@RequestParam(value = "talukId", required = false, defaultValue = "0") long talukId,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") long localBodyId,
			RedirectAttributes redirectAttributes, @RequestParam(value = "vehicleTypeId") long vehicleTypeId,
			@RequestParam(value = "mobile") String mobile, @RequestParam(value = "purposeId") long purposeId,
			@RequestParam(value = "typeId") long typeId, @RequestParam(value = "mobileOfOwner") String mobileOfOwner,
			@RequestParam(value = "mobileOfDriver") String mobileOfDriver,
			@RequestParam(value = "helperAddress") String helperAddress,
			@RequestParam(value = "helperMobile") String helperMobile) {
		essentialService.setLocalBody(masterService.getLocalBody(localBodyId));
		essentialService.setTaluk(masterService.getTaluk(talukId));
		essentialService.setMobile(mobile);
		essentialService.setPermitType(coronaService.getPermitType(typeId));
		essentialService.setServicePurpose(coronaService.getServicePurpose(purposeId));
		essentialService.setVehicleType(coronaService.getVehicleType(vehicleTypeId));
		essentialService.setAddressOfDriver(essentialService.getAddressOfDriver() + ", " + mobileOfDriver);
		essentialService.setAddressOfOwner(essentialService.getAddressOfOwner() + ", " + mobileOfOwner);
		essentialService.setGoingWith(essentialService.getGoingWith() + ", " + helperMobile + ", " + helperAddress);
		coronaService.saveEssentialService(essentialService);
		String msgContent = "";
		try {
			msgContent = "Covid 19 Jagratha: Your application has been send for processing. You will receive SMS after approval. You can check status by below link\n"
					+ "https://covid19jagratha.kerala.nic.in/home/viewStatus?serviceId=" + URLEncoder.encode(
							commonService.encryptMessage(Long.toString(essentialService.getServiceId())), "UTF-8");
		} catch (UnsupportedEncodingException e) {
			e.printStackTrace();
		}
		commonService.sendSMS(essentialService.getMobile(), msgContent, false);
		redirectAttributes.addFlashAttribute("message",
				"Your application has been send for processing. You will receive SMS after approval.");
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "downloadService")
	public void downloadService(@RequestParam(value = "serviceId") String serviceId, HttpServletResponse response,
			HttpServletRequest request) {

		EssentialService essentialService = coronaService
				.getEssentialService(Long.parseLong(commonService.decryptMessage(serviceId)));
		String content = "<h1>Permission Expried</h>";
		if (essentialService != null) {
			if (essentialService.getStatus() == 'A') {
				if (essentialService.getAttachment() == null) {
					String issuing = "";
					if (essentialService.getUserDetails().getType().equals("LB")) {
						LocalBody localBody = masterService.getLocalBody(essentialService.getUserDetails());
						issuing += "SECRETARY<br>" + localBody.getName().toUpperCase() + "<br>PHONE: "
								+ localBody.getMobile();
					} else if (essentialService.getUserDetails().getType().equals("TO")) {
						Taluk taluk = masterService.getTaluk(essentialService.getUserDetails());
						issuing += "TALUK CONTROL OFFICER <br>" + taluk.getName().toUpperCase() + "<br>PHONE: "
								+ taluk.getMobile();
					} else if (essentialService.getUserDetails().getType().equals("DC")) {
						Officer officer = masterService.getOfficer(essentialService.getUserDetails());
						issuing += "DISTRICT COLLECTOR <br>" + officer.getDistrict().getDistName().toUpperCase();
					}

					String qrContent = "";
					try {
						qrContent = "https://covid19jagratha.kerala.nic.in/home/verifyEssentialService?serviceId="
								+ URLEncoder.encode(
										commonService.encryptMessage(Long.toString(essentialService.getServiceId())),
										"UTF-8");
					} catch (UnsupportedEncodingException e1) {
						e1.printStackTrace();
					}
