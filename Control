	@RequestMapping(value = "savePermission", method = RequestMethod.POST)
	public ModelAndView savePermission(@ModelAttribute("permission") Permission permission,
			HttpServletResponse response, HttpServletRequest request,
			@RequestParam(value = "localBodyId", required = false, defaultValue = "0") 
			long localBodyId, RedirectAttributes redirectAttributes, 
			@RequestParam(value = "vehicleTypeId", required = false, defaultValue = "0") long vehicleTypeId,
			@RequestParam(value = "mobile") String mobile,@RequestParam(value = "localBodyIdTo", 
			required = false, defaultValue = "0") long localBodyIdTo) {
		
		if(otherService.countofOffence(mobile)== 0 && 
				!checkValidatedOtp(request.getSession(), request.getParameter("userOtp"))) {
			redirectAttributes.addFlashAttribute("message", "Invalid Otp");
			return new ModelAndView("redirect:/home/addpermission");
		}
		
		if (ESAPI.validator().isValidInput("Mobile", permission.getMobile(), "Mobile", 10, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Mobile No");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("Name", permission.getName(), "GivenName",20, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Name");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("localBodyId", String.valueOf(localBodyId), "Num", 4, false) == false
				|| localBodyId == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Localbody From");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("localBodyIdTo", String.valueOf(localBodyIdTo), "Num", 4, false) == false
				|| localBodyIdTo == 0) {
			redirectAttributes.addFlashAttribute("message", "Invalid Localbody To");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("From", permission.getFrom(), "WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Place From. Only alphabets, digits, space and hyphen are allowed (50 characters max)");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("To", permission.getTo(), "WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Place From. Only alphabets, digits, space and hyphen are allowed (50 characters max)");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("Address", permission.getAddress(), "WordSS", 200, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid address. "
					+ "Only alphabets, digits, space and the following special characters "
					+ "are allowed(-_,:/@#.$!*'%^()) (200 characters max)");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("Vehicle Type", String.valueOf(vehicleTypeId), "Num", 4, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Vehicle Type");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("Vehicle Number", String.valueOf(permission.getVehicleNo()),
				"WordSWithHyphen", 15, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Vehicle Number. Only alphabets, digits, space and hyphen are allowed (15 characters max)");
			return new ModelAndView("redirect:/home/addpermission");
		}
		if (ESAPI.validator().isValidInput("Purpose", String.valueOf(permission.getPurpose()), 
				"WordSWithHyphen", 50, false) == false) {
			redirectAttributes.addFlashAttribute("message", "Invalid Purpose. Only alphabets, digits, space and hyphen are allowed (15 characters max)");
			return new ModelAndView("redirect:/home/addpermission");
		}
		
		if(permission.getGoingDate() != null && permission.getReturnBack() != null) {
			if(permission.getGoingDate().compareTo(permission.getReturnBack()) < 0) {
				try {
					SimpleDateFormat sdf = new SimpleDateFormat("E MMM dd HH:mm:ss z yyyy");
					sdf.setLenient(false);
					sdf.parse(String.valueOf(String.valueOf(permission.getGoingDate())));
					sdf.parse(String.valueOf(String.valueOf(permission.getReturnBack())));
				}catch(Exception ex){
					redirectAttributes.addFlashAttribute("message", "Invalid Date");
					return new ModelAndView("redirect:/home/addpermission");
				}
			}
			else {
				redirectAttributes.addFlashAttribute("message", "Invalid Date. Return back date should be greater than going date");
				return new ModelAndView("redirect:/home/addpermission");
			}
		}else {
			redirectAttributes.addFlashAttribute("message", "Invalid Date");
			return new ModelAndView("redirect:/home/addpermission");
		}
		
		permission.setLocalBody(masterService.getLocalBody(localBodyId));
		permission.setLocalBodyTo(masterService.getLocalBody(localBodyIdTo));
		permission.setMobile(mobile);
		permission.setVehicleType(coronaService.getVehicleType(vehicleTypeId));
		coronaService.savePermission(permission);
		String qrContent = "";
		try {
			qrContent = "Download self declaration from below link"
					+ "\nThis should be shown to enforcement squads. Vioaltion of self declaration will be viewed seriously and action will be taken."
					+ "\n-DISTRICT COLLECTOR " + permission.getLocalBody().getDistrict().getDistName() + "\n"
					+ "https://covid19jagratha.kerala.nic.in/home/downloadPermission?permissionId=" + URLEncoder
							.encode(commonService.encryptMessage(Long.toString(permission.getPermissionId())), "UTF-8");
		} catch (UnsupportedEncodingException e) {
			e.printStackTrace();
		}
		commonService.sendSMS(mobile, qrContent, false);

		redirectAttributes.addFlashAttribute("message",
				"Your self declaration is saved and a link sent as SMS to your number.");
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "downloadPermission")
	public void downloadPermission(@RequestParam(value = "permissionId") String permissionId,
			HttpServletResponse response, HttpServletRequest request) {

		Permission permission = coronaService.getPermission(Long.parseLong(commonService.decryptMessage(permissionId)));
		String content = "";

		if (permission != null) {
			String alone = permission.getGoingWith();
			if (alone.equals("")) {
				alone = "alone";
			} else {
				alone = "along with " + permission.getGoingWith();
			}
			content = "<h1>Permission Expried</h>";

			String qrContent = "";
			try {
				qrContent = "https://covid19jagratha.kerala.nic.in/home/verifyPermission?permissionId=" + URLEncoder
						.encode(commonService.encryptMessage(Long.toString(permission.getPermissionId())), "UTF-8");
			} catch (UnsupportedEncodingException e1) {
				e1.printStackTrace();
			}
			String qr = "<img alt=\"qr\" width=\"80\" height=\"60\" src=\"data:image/png;base64,"
					+ Base64.getEncoder().encodeToString(commonService.generateQRCode(qrContent)) + "\"/>";
			String toLocal = "";
			if (permission.getLocalBodyTo() != null) {
				toLocal = " (" + permission.getLocalBodyTo().getName() + ")";
			}
			if (permission.getReturnBack().compareTo(new Date()) >= 0) {
				content = "<div style=\"font-size: 12pt; max-height: 29.7cm; margin: 2mm 6mm 8mm 6mm;\">"
						+ "<p \"text-align: left;\">Application No: " + permission.getPermissionId()
						+ "</p><p style=\"text-align: center; font-weight: bold; font-size: 13pt\">"
						+ "<u>SELF DECLARATION</u></p>" + "<p style=\"text-align: justify; line-height: 2em\">"
						+ "<span style=\"margin-left: 4em\"></span>I, <b>" + permission.getName() + "</b>, "
						+ permission.getAddress() + ", travelling in vehicle " + "number <b>"
						+ permission.getVehicleNo() + "</b> (" + permission.getVehicleType().getName()
						+ "). I am going " + alone + " from " + permission.getFrom() + " ("
						+ permission.getLocalBody().getName() + ") to <b>" + permission.getTo() + toLocal
						+ "</b> for the purpose of " + permission.getPurpose() + ", which is unavoidable."
						+ "</p><br /><p>I will return on <b>"
						+ new SimpleDateFormat("dd/MM/yyyy hh:mm aa").format(permission.getReturnBack()) + "</b></p>"
						+ "<br /> <br /><div style=\"text-align: left; float: left;\">" + "Date & Time: "
						+ new SimpleDateFormat("dd/MM/yyyy hh:mm aa").format(permission.getGoingDate())
						+ "<br /> Place: " + permission.getLocalBody().getDistrict().getDistName() + "</div>"
						+ "<div style=\"text-align: left; float: right\">" + "Name: " + permission.getName()
						+ "<br /> Mobile number: " + permission.getMobile()
						+ "</div><p style=\"text-align: justify; line-height: 2em\"><br><br>Note: This is a "
						+ "Self Declaration only and it cannot be used to cross "
						+ "the District borders. Please note that frivolous self-declaration and "
						+ "violation of self-declaration will be viewed seriously and strict action "
						+ "will be taken.</p><br><br><div>" + qr + "</div></div>";
			}
			try {
				ByteArrayOutputStream byteArrayOutputStream = new ByteArrayOutputStream();
				InputStream inputStream = new ByteArrayInputStream(content.getBytes());
				HtmlConverter.convertToPdf(inputStream, byteArrayOutputStream);
				response.setContentType("application/pdf");
				response.setHeader("Content-Disposition", "attchment;filename=\"Self Declaration.pdf\"");
				FileCopyUtils.copy(byteArrayOutputStream.toByteArray(), response.getOutputStream());
			} catch (Exception e) {
				e.printStackTrace();
			}
		} else {
			content = "<h1>Covid 19 Jagratha: Invalid Self Declaration.</h>";
			try {
				ByteArrayOutputStream byteArrayOutputStream = new ByteArrayOutputStream();
				InputStream inputStream = new ByteArrayInputStream(content.getBytes());
				HtmlConverter.convertToPdf(inputStream, byteArrayOutputStream);
				response.setContentType("application/pdf");
				response.setHeader("Content-Disposition", "attchment;filename=\"Vehicle Permit.pdf\"");
				FileCopyUtils.copy(byteArrayOutputStream.toByteArray(), response.getOutputStream());
			} catch (Exception e) {
				e.printStackTrace();
			}
		}

	}

