	@RequestMapping(value = "getForgotNORKA")
	public ModelAndView getForgotNORKA(@RequestParam(value = "mobile") String mobile,
			@RequestParam(value = "dob", required = false, defaultValue = "0") String dob,
			RedirectAttributes redirectAttributes, @RequestParam("captchaCode") String captcha,
			HttpSession session) {
		if (!session.getAttribute("captcha_security").equals(captcha)) {
			
			redirectAttributes.addFlashAttribute("message", "Invalid captcha!");
			return new ModelAndView("redirect:/home/addForgotNORKA");
			
		}
		session.removeAttribute("captcha_security");
		Date date = new Date();
		DateFormat format = new SimpleDateFormat("dd-MM-yyyy");
		try {
			date = format.parse(dob);
		} catch (ParseException e) {
			e.printStackTrace();
		}
		Domestic domestic = domesticService.getDomestic(mobile, date);
		if (domestic != null) {
			commonService.sendSMS("Covid 19 Jagratha: " + domestic.getMobile(),
					domestic.getRegisterNumber() + " is the Register Number in NORKA.", true);
			redirectAttributes.addFlashAttribute("message",
					"Register Number is send to your corresponding NORKA Registred Mobile Number");
			return new ModelAndView("redirect:/home/");
		} else {
			redirectAttributes.addFlashAttribute("message", "Invalid date of birth or mobile number");
			return new ModelAndView("redirect:/home/addForgotNORKA");
		}
	}

