	@RequestMapping(value = "countVehiclePermit", method = RequestMethod.POST)
	@ResponseBody
	public long countVehiclePermit(@RequestParam("mobile") String mobile,
			@RequestParam("captchaCode") String captchaCode,HttpSession session) {
		
		if (session.getAttribute("captcha_security").equals(captchaCode)) {
			if(coronaService.countVehiclePermit(mobile)>0) {
				sendMessage(session, mobile);
				return 1;
			}else {
				return 0;
			}
		}
		else {
			session.removeAttribute("captcha_security");
			return -1;
		}
		
	}

