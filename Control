	@RequestMapping(value = "changeDomseticDetails", method = RequestMethod.POST)
	@ResponseBody
	public List<Domestic> changeDomseticDetails(@RequestParam(value = "domesticId") long domesticId,
			@RequestParam(value = "mobile") String mobile, @RequestParam(value = "name") String name) {
		Domestic domestic = domesticService.getDomestic(domesticId);
		domestic.setName(name);
		domestic.setMobile(mobile);
		domesticService.saveDomestic(domestic);
		return domesticService.listDomestic(domestic.getHead());
	}

	@RequestMapping(value = "deleteDomseticDetails", method = RequestMethod.POST)
	@ResponseBody
	public List<Domestic> deleteDomseticDetails(@RequestParam(value = "domesticId") long domesticId) {
		Domestic domestic = domesticService.getDomestic(domesticId);
		Domestic head = domestic.getHead();
		domestic.setHead(null);
		domesticService.saveDomestic(domestic);
		List<Domestic> list = domesticService.listDomestic(head);
		return list;
	}

	@RequestMapping(value = "updateDomestic", method = RequestMethod.POST)
	public ModelAndView updateDomestic(@ModelAttribute("domestic") Domestic domestic1,
			@RequestParam(value = "stationId", required = false, defaultValue = "0") long stationId,
			@RequestParam(value = "trainId", required = false, defaultValue = "0") long trainId,
			@RequestParam(value = "coachId", required = false, defaultValue = "0") long coachId,
			RedirectAttributes redirectAttributes,
			@RequestParam(value = "timeSlotId", required = false, defaultValue = "0") long timeSlotId) {
		TimeSlot timeSlot = domesticService.getTimeSlot(timeSlotId);
		Domestic domestic = domesticService.getDomestic(domestic1.getDomesticId());
		domestic.setTrain(domesticService.getTrain(trainId));
		domestic.setRailwayStation(domesticService.getRailwayStation(stationId));
		domestic.setCoach(domesticService.getCoach(coachId));
		domestic.setPnr(domestic1.getPnr());
		domestic.setDepartingDate(domestic1.getDepartingDate());
		domestic.setVehicleNo(domestic1.getVehicleNo());
		if (timeSlot != null) {
			domestic.setTravelDate(domestic1.getTravelDate());
			domestic.setTimeSlot(timeSlot);
		}
		List<Domestic> list = domesticService.listDomestic(domestic);
		Set<Domestic> members = new HashSet<>();
		for (Domestic domestic2 : list) {
			domestic2.setTravelDate(domestic.getTravelDate());
			domestic2.setVehicleNo(domestic.getVehicleNo());
			domestic2.setTimeSlot(domestic.getTimeSlot());
			domestic2.setTrain(domestic.getTrain());
			domestic2.setRailwayStation(domestic.getRailwayStation());
			domestic2.setCoach(domestic.getCoach());
			members.add(domestic2);
		}
		members.add(domestic);
		if (timeSlot != null) {
			if (domestic.getVehicleCapacity().getCapacity() != 0) {
				if (members.size() > timeSlot.getCapacity()) {
					redirectAttributes.addAttribute("domesticId", domestic.getDomesticId());
					redirectAttributes.addFlashAttribute("message",
							"Time slot you choosen is filled. Please choose another.");
					return new ModelAndView("redirect:/home/viewDomestic");
				}
			}
		}
		domesticService.saveDomestic(members);
		if (timeSlot != null) {
			if (domestic.getVehicleCapacity().getCapacity() != 0) {
				timeSlot.setCapacity(timeSlot.getCapacity() - members.size());
				domesticService.saveTimeSlot(timeSlot);
			}
		}
		String msgContent = "";
		msgContent = "Covid 19 Jagratha: Your application with number " + domestic.getDomesticId()
				+ " is updated successfully.";
		commonService.sendSMS(domestic.getMobile(), msgContent, false);
		redirectAttributes.addFlashAttribute("message",
				"Your application is edited. Your registration number is " + domestic.getDomesticId());
		return new ModelAndView("redirect:/home/");
	}

	@RequestMapping(value = "getTrain", method = RequestMethod.POST)
	@ResponseBody
	public List<Train> getTrain(@RequestParam(value = "travelDate") String travelDate) {

		Date date = new Date();
		DateFormat format = new SimpleDateFormat("dd-MM-yyyy");
		try {
			date = format.parse(travelDate);
		} catch (ParseException e) {
			e.printStackTrace();
		}

		return domesticService.listTrain(date);
	}
